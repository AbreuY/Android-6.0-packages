<?xml version="1.0" encoding="utf-8"?>
<!-- Sunvov:wlj 20150820 modify for **** start @{ -->
<LinearLayout android:orientation="vertical"
	android:background="#000000" android:paddingRight="45dip"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
<!-- Sunvov:wlj 20150820 modify for **** end @} -->
	<!-- Sunvov:wlj 20150725 modify for **** start @{ -->
	<LinearLayout android:id="@+id/time_setting"
		android:layout_gravity="center" android:paddingTop="23dip"
		android:layout_marginStart="22dp"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
	<!-- Sunvov:wlj 20150725 modify for **** end @} -->
		<ImageView android:id="@+id/hour_high" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:src="@drawable/time_number_00" />
		<ImageView android:id="@+id/hour_low" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:src="@drawable/time_number_00" />
		<ImageView android:id="@+id/hour_min_sepretor"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:src="@drawable/time_sepretor" />
		<ImageView android:id="@+id/min_high" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:src="@drawable/time_number_00" />
		<ImageView android:id="@+id/min_low" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:src="@drawable/time_number_00" />
		<ImageView android:id="@+id/time_formate" android:paddingTop="30.0dip"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:src="@drawable/am" />
	</LinearLayout>
	<!-- Sunvov:wlj 20150725 modify for **** start @{ -->
	<LinearLayout android:id="@+id/date_setting"
		android:layout_gravity="center" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:paddingTop="30dip">
		<!-- Sunvov:wlj 20150725 modify for **** end @} -->
		<ImageView android:id="@+id/date_first" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:src="@drawable/date_number_00" />
		<ImageView android:id="@+id/date_second"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:src="@drawable/date_number_00" />
		<ImageView android:id="@+id/date_sepretor"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:src="@drawable/date_sepretor" />
		<ImageView android:id="@+id/date_third" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:src="@drawable/date_number_00" />
		<ImageView android:id="@+id/date_forth" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:src="@drawable/date_number_00" />
		<ImageView android:id="@+id/week"
			android:paddingTop="2.0dip" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:src="@drawable/week_mon" />
	</LinearLayout>
</LinearLayout>